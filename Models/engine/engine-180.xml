<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>engine-180.ac</path>

  <animation>
    <type>spin</type>
    <object-name>ring-gear</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> -2.3 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.0 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>select</type>
    <object-name>ring-gear</object-name>
    <condition>
	<less-than>
    	<property>engines/engine[0]/rpm</property>
	<value>1500</value>
	</less-than>
    </condition>
  </animation>

</PropertyList>


